<hibernate-mapping>

    <class name="com.yz.manager.custom.bean.customState" table="customState">
    
        <id name="id" column="id" type="int">
        	<generator class="native" />
        </id>
     
        <property name="stateName" /> 
    </class>

</hibernate-mapping>
