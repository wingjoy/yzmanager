<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="utf-8"/>	
	 <constant name="struts.multipart.saveDir" value="/tempFolder"></constant>  
	<package name="strutsfile" extends="struts-default" namespace="/">
		<action name="fileUpload_*" class="com.yz.manager.action.UploadAction" method="{1}">
			<result name="uploadSuccess" type="redirectAction">/uploadfile/upload.jsp</result>
			<result name="uploadFiled" type="redirectAction">/uploadfile/upload.jsp</result>
			<result name="listSuccess" type="redirectAction">/uploadfile/filelist.jsp</result>
			<result name="input" >/uploadfile/upload.jsp</result>
		</action>
	</package>
</struts>