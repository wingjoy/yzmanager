<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="utf-8"/>	
	<package name="strutsfile" extends="struts-default">
		<action name="fileUpload" class="com.yz.manager.action.UploadAction">
			<result name="uploadSuccess" type="dispatcher">/uploadfile/upload.jsp</result>
			<result name="uploadFiled" type="dispatcher">/uploadfile/upload.jsp</result>
			<result name="listSuccess" type="dispatcher">/uploadfile/filelist.jsp</result>
		</action>
	</package>
</struts>